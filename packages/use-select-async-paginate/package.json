{
  "name": "use-select-async-paginate",
  "version": "1.0.0",
  "description": "Model for asynchronous selects for support pagination on menu scroll",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "react-select",
    "async",
    "pagination",
    "fetch"
  ],
  "repository": "git@github.com:vtaits/react-select-async-paginate.git",
  "homepage": "https://github.com/vtaits/react-select-async-paginate/tree/master/packages/use-select-async-paginate",
  "author": "Vadim Taits",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "prepare": "yarn build",
    "test:ts": "tsc --noEmit",
    "test:unit": "jest",
    "test:unit:cov": "jest --coverage",
    "codecov": "codecov -F select_async_paginate_model"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "select-async-paginate-model": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
